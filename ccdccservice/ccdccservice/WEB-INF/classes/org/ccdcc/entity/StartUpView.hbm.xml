<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 
   administrator
-->
<hibernate-mapping package="org.ccdcc.entity">
	<class name="StartUpView" table="StartUpTable" mutable="false" schema="dbo" >
		<id name="id" column="id" type="java.lang.Integer">
			<generator class="native"></generator>
		</id>
		
		<property name="beginAddress" type="java.lang.String"></property>
		<property name="beginTime" type="java.util.Date"></property>
		<property name="carrier" type="java.lang.String"></property>
		<property name="endAddress" type="java.lang.String"></property>
		<property name="endTime" type="java.util.Date"></property>
		<property name="receiver" type="java.lang.String"></property>
		<property name="recordInterval" type="java.lang.Integer"></property>
		<property name="shipper" type="java.lang.String"></property>
		<property name="updateStatus" type="java.lang.Integer"></property>
		<property name="tlimitType" type="java.lang.Integer"></property>
		<property name="tuplimit" type="java.lang.Integer"></property>
		<property name="tdwlimit" type="java.lang.Integer"></property>
	</class>
</hibernate-mapping>