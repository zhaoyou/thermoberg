<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 
    administrator
-->
<hibernate-mapping package="org.ccdcc.entity">
    <class name="CarRealView" table="tbcccarrealview" schema="dbo" mutable="false">
        
       <id name="projectId" type="java.lang.String" column="projectId">
       	<generator class="assigned"></generator>
       </id>
        
        <property name="id" type="java.lang.Integer" column="id"></property>
        <property name="ai1"></property>
        <property name="ai2"></property>
        <property name="ai3"></property>
        <property name="ai4"></property>
        
        <property name="latitude"></property>
        <property name="latitude_dir"></property>
        <property name="longitude"></property>
        <property name="longitude_dir"></property>
        
        <property name="updateTime"			 type="java.util.Date" column="UpdateTime"></property>
        <property name="runStatus"			 type="java.lang.Integer" column="RunStatus"></property>
        <property name="alarmStatus"		 type="java.lang.Integer" column="AlarmStatus"></property>
        <property name="connectionStatus"	 type="java.lang.Integer" column="ConnectStatus"></property>
        
    </class>
</hibernate-mapping>
