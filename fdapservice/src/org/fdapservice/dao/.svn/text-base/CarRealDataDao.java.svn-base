package org.fdapservice.dao;

import org.apache.log4j.Logger;

/**
 * 车载实时数据上传数据访问接口
 * @author liaodd
 */

public interface CarRealDataDao {
	
	public static final String UPLOADCAR = "{call proc_loadCarRealData(?,?,?,?)}";
	
	/**
	 * 车载实时数据上传
	 * @param code				企业授权码
	 * @param psource			源数据{aid1,aid2,aid3,t1,t2,t3,latitude,latitude_dit,longitude,longitude_dir,time}
	 * @return					上传的操作状态 0 成功 1 失败 2 服务器发生错误
	 */
	public Integer uploadCarReal(String code,String psource);
	
	/**
	 * 记录日志
	 * @param result			操作状态值
	 * @param msg				操作信息
	 * @param log				日志对象
	 */
	public void addLog(Integer result,String msg,Logger log);
}
