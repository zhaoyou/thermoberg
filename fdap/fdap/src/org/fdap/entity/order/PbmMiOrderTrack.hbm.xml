<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 
    zhaoyou
-->
 
<hibernate-mapping>
    <class name="org.fdap.entity.order.PbmMiOrderTrack" table="pbmOrderTrack" >
        <id name="orderId" type="java.lang.Long">
            <column name="orderId" />
            <generator class="identity"/>
        </id>  
  
        <property name="receiverId" type="java.lang.Long" />
        <property name="orderId" type="java.lang.Long" />
        <property name="orderTime" type="java.util.Date" />
        <property name="isDelete" type="java.lang.Short"/>
         <property name="uploadStatus" type="java.lang.Short"/>
        <property name="oid" type="java.lang.Long" />
    </class>
</hibernate-mapping>
