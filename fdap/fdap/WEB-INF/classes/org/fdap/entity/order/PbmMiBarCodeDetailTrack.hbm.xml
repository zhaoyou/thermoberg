            <column name="bcdId" />
            <generator class="identity"/>
        </id>  
        <property name="bcId" type="java.lang.Long" />
        <property name="orderId" type="java.lang.Long" />
        <property name="goodsFullId" type="java.lang.Long" />
        <property name="totalNum" type="java.lang.Integer" />
        <property name="kid" type="java.lang.Long" />
        <property name="isDelete" type="java.lang.Short" />
        <property name="oid" type="java.lang.Long" />
    </class>
