<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 
    zhaoyou
-->
 
<hibernate-mapping>
    <class name="org.fdap.entity.order.PbmMiGoodsBaseInfo" table="pbmGoodsBasicInfo" >
        <id name="goodId" type="java.lang.Long">
            <column name="goodId" />
            <generator class="identity"/>
        </id>  
  
        <property name="goodsName" type="java.lang.String" />
        <property name="goodsType" type="java.lang.String" />
        <property name="goodsUnit" type="java.lang.String" />
        <property name="typeName" type="java.lang.String" />
        <property name="prodarea" type="java.lang.String" />
        <property name="storageEnv" type="java.lang.String" />
        
        <property name="oid" type="java.lang.Long" />
        <property name="isDelete" type="java.lang.Short"></property>
    </class>
</hibernate-mapping>