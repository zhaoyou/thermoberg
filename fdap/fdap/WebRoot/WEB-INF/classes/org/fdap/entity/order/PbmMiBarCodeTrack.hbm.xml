<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 
    zhaoyou
-->
 
<hibernate-mapping>
		<class name="org.fdap.entity.order.PbmMiBarCodeTrack" table="pbmBarcodeTrack" >
        <id name="bcId" type="java.lang.Long">
            <column name="bcId" />
            <generator class="identity"/>
        </id>  
  
        <property name="orderId" type="java.lang.Long" />
        <property name="barCode"  type="java.lang.String"/>
        <property name="barCodeType" type="java.lang.Short" />
        
        <property name="packetType" type="java.lang.Integer" />
        <property name="packetNum" type="java.lang.Integer" />
        
        <property name="isDelete" type="java.lang.Short" />
        <property name="oid" type="java.lang.Long" />
    </class>
    
</hibernate-mapping>
